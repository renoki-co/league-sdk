{
    "name": "renoki-co/league-sdk",
    "description": "League SDK is a PHP-based SDK that works with RIOT Games API in an object-oriented manner.",
    "keywords": ["laravel", "php", "riot", "games", "sdk", "league", "of", "legends", "datadragon"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/league-sdk",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/cache": "^7.30|^8.23",
        "illuminate/support": "^7.30|^8.23"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LeagueSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LeagueSdk\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LeagueSdk\\LeagueSdkServiceProvider"
            ]
        }
    }
}
